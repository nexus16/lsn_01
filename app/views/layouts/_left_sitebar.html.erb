<div class="card-panel teal lighten-2 margin-5">
  <%= t "button.find_by_class" %>
</div>
<div class="row list-class" >
  <% @supports.list_class.each do |c| %>
    <div class="col m4 class-item">
      <div class="card">
        <div class="card-panel blue"><%= c.name %></div>
        <div class="row">
          <% c.descendants.each do |descendant| %>
            <div class="col m4 ">
              <div class="card-panel grey">
                <%= link_to descendant.name, descendant, class: "text-white",
                  "data-turbolinks": false %>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  <% end %>
</div>
<div class="card-panel teal lighten-2 margin-5">
  <%= t "button.find_by_object" %>
</div>
<div class="row list-class">
  <% @supports.list_object.each do |object| %>
    <div class="col m4">
      <div class="card-panel blue"><%= object %></div>
    </div>
  <% end %>
</div>
